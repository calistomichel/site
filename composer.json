{
    "name": "michelcalisto/site",
    "description": "Tailwind Site scaffolding for the Laravel framework using inertia stack.",
    "keywords": ["laravel", "site"],
    "homepage": "https://github.com/michelcalisto/site",
    "license": "MIT",
    "authors": [
        {
            "name": "Michel Calisto",
            "email": "calisto.michel@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "MichelCalisto\\Site\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MichelCalisto\\Site\\SiteServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
