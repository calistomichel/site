{
    "name": "calistomichel/site",
    "description": "Site scaffolding for the Laravel framework using inertia stack.",
    "keywords": ["laravel", "site"],
    "homepage": "https://github.com/calistomichel/site",
    "license": "MIT",
    "authors": [
        {
            "name": "Calisto Michel",
            "email": "calisto.michel@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "CalistoMichel\\Site\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "CalistoMichel\\Site\\SiteServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
